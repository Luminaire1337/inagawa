// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./inagawa.db"
}

model Warehouse {
  id              Int       @id @default(autoincrement())
  remoteId        Int?      @unique()
  vehicleName     String
  vehicleId       Int?
  importerName    String
  currentPrice    Int
  exported        Boolean   @default(false)
  exporterName    String?
  exportedPrice   Int?
}

model Log {
  id              Int       @id @default(autoincrement())
  remoteId        Int       @unique()
  authorName      String
  message         String
  time            DateTime  @default(now())
}